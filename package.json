{
  "name": "throttling-api-dashboard",
  "version": "0.0.1",
  "description": "nodejs reactjs fullstack",
  "main": "index.ts",
  "scripts": {
    "dev": "NODE_ENV=development VERBOSE='1' nodemon index.ts",
    "start": "NODE_ENV=production nodemon index.ts",
    "start:compile": "concurrently \"tsc -w\" \"nodemon build/server/index.js\"",
    "build": "tsc --project ./",
    "test:init": "ts-jest config:init",
    "test:server": "ENV_FILE=./.env.test jest ./server",
    "lint": "eslint *.ts",
    "format": "eslint *.ts --fix"
  },
  "author": "Ikhlas Risandy",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
